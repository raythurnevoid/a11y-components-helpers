{
	"name": "@raythurnevoid/a11y-components-helpers",
	"version": "0.0.1",
	"description": "Helps with building accessible components",
	"scripts": {
		"dev": "vite dev",
		"dev:inspect": "node --inspect-brk ./node_modules/vite/bin/vite.js",
		"playground:build": "vite build",
		"build": "vite build --config ./vite.config.build.js",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"package": "npm run build && copyfiles ./package.json ./dist && publint ./dist",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest"
	},
	"type": "module",
	"exports": {
		"./autocomplete/*": "./autocomplete/*",
		"./dialog/*": "./dialog/*"
	},
	"workspaces": [
		"./website"
	],
	"devDependencies": {
		"@fontsource/roboto": "^4.5.8",
		"@material/icon-button": "^14.0.0",
		"@material/top-app-bar": "^14.0.0",
		"@material/typography": "^14.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.15.2",
		"@vitest/browser": "^0.30.1",
		"copyfiles": "^2.4.1",
		"playwright": "^1.33.0",
		"prettier": "^2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"publint": "^0.1.11",
		"rollup-plugin-dts": "^5.3.0",
		"sass": "^1.61.0",
		"svelte": "^3.58.0",
		"svelte-check": "^3.2.0",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vite-plugin-dts": "^2.2.0",
		"vitest": "^0.30.1",
		"webdriverio": "^8.8.8"
	}
}
